def trap(height):
    if not height:
        return 0
    
    left = 0
    right = len(height) - 1
    left_max = right_max = 0
    trapped_water = 0
    
    while left < right:
        if height[left] < height[right]:
            if height[left] >= left_max:
                left_max = height[left]
            else:
                trapped_water 
